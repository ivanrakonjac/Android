<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/bottom_navigation_workouts"
    app:startDestination="@id/workoutListFragment"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/workoutListFragment"
        android:name="ika.test.runningapp.workouts.WorkoutListFragment"
        android:label="fragment_workout_list"
        tools:layout="@layout/fragment_workout_list" >
        <action
            android:id="@+id/action_workoutListFragment_to_workoutCreateFragment"
            app:destination="@id/workoutCreateFragment" />
    </fragment>
    <fragment
        android:id="@+id/workoutCreateFragment"
        android:name="ika.test.runningapp.workouts.WorkoutCreateFragment"
        android:label="fragment_workout_create"
        tools:layout="@layout/fragment_workout_create" />
</navigation>